// Challenge 3 : Somme des Éléments
#include <stdio.h>
#include <string.h>

int x,y,z,i,j,r,h,u;
int max;


int main(){
    printf("entrer n:");
    scanf("%d",&j);
    int T[j];
    x = 0;

    for (i = 0;i < j;i++){
            printf("entrer T[%d] = :",i+1);
            scanf("%d",&T[i]);

    }
    for (i = 0;i < j;i++){
            x = x+T[i];


    }



    printf("la somme de tableau est %d:",x);






    return 0;

}

